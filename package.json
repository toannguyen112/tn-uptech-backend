{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "app.ts",
  "repository": "https://github.com/toannguyen112",
  "author": "toannguyen112",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon server.ts",
    "build": "tsc -b",
    "start": "node --max-old-space-size=2048 dist/server.js",
    "test": "jest",
    "seeder": "ts-node src/assets/import/index.ts",
    "migration": "sequelize-cli db:migrate",
    "un_migration": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "reset": "sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "migrate:dev": "yarn run build && cross-env NODE_ENV=development dotenv -e .env sequelize db:migrate",
    "migrate:prod": "yarn run build && cross-env NODE_ENV=production dotenv -e .env sequelize db:migrate --env production"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/validator": "^13.7.4",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.22.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "i18next": "^22.4.11",
    "i18next-express-middleware": "^2.0.0",
    "i18next-node-fs-backend": "^2.1.3",
    "inflection": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "pg": "^8.7.3",
    "pm2": "^5.3.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.8.1",
    "sequelize": "^6.29.3",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.4",
    "typedi": "^0.10.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.12",
    "nodemon": "^2.0.18",
    "sucrase": "^3.23.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  }
}
